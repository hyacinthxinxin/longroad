<%= form_for(@building) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.label :name, '项目的名字' %>
    <%= f.text_field :name, placeholder: "建筑的名字." %>
  </div>
  <div class="field">
    <%= f.label :socket_address, '项目的端口地址' %>
    <%= f.text_field :socket_address, placeholder: "建筑的地址." %>
  </div>
  <%= f.submit "确定", class: "btn btn-primary" %>
<% end %>